VITE_OUTCOME_ROWS=7
# Obtain from CMC dashboard; keep private and out of repo
VITE_CMC_API_KEY=YOUR_CMC_API_KEY
# Optional: control home overlay countdown
# VITE_COUNTDOWN_TARGET=2025-10-09T20:39:43.902Z
# WalletConnect Cloud Project ID for QR-based wallet connection (optional)
# Sign up at https://cloud.walletconnect.com to obtain this value
VITE_WALLETCONNECT_PROJECT_ID=
# Toggle wallet guard on dApp pages. When set to 'false',
# dApp pages render without requiring a wallet connection and
# components may use a preview fallback address for display.
VITE_ENABLE_WALLET_GUARD=true

# Default sponsor address used when no referral is present or expired
VITE_DEFAULT_SPONSOR_ADDRESS=